            VALUE "ProductName",     kTCAT_DICE_USR_STR
            VALUE "ProductVersion",  kTCAT_DICE_VERSION_STR
        END

    END

    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 0x04B0
    END
END

